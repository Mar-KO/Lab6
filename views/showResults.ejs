<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>Likes</h2>
    <ul>
    <% for (var i=0; i<results[0].length; i++){ %>
        <li><%= results[0][i].like %> :  <%= results[0][i].count %></li>
    <% } %>    
    </ul>
    <h2>Gender</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].gender %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>
    <h2>Flaws</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].flaws %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>
    <h2>Comments</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].comment %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>
    <h2>Field</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].field %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>
    <h2>Recommendations</h2>
    <ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].recommendation %> :  <%= results[5][i].count %></li>
    <% } %>    
    </ul>

   
</body>
</html>